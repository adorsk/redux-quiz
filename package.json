{
  "name": "redux-webk-boilerplate",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "npm run clean && mkdir -p build && cp index.html build/ && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack --config webpack/webpack.conf.build.js",
    "clean": "rimraf build",
    "lint": "eslint src",
    "start": "node devServer.js",
    "test": "BABEL_ENV=testing karma start karma/karma.conf.js",
    "test:watch": "npm run test -- --no-single-run"
  },
  "babel": {
    "stage": 0,
    "env": {
      "development": {
        "plugins": ["react-transform"],
        "extra": {
          "react-transform": {
            "transforms": [{
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }, {
              "transform": "react-transform-catch-errors",
              "imports": ["react", "redbox-react"]
            }]
          }
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adorsk/react-bt-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "hot",
    "reload",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform",
    "karma"
  ],
  "author": "adorsk (http://github.com/adorsk)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/adorsk/redux-bt-boilerplate/issues"
  },
  "homepage": "https://github.com/adorsk/redux-bt-boilerplate",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.1.1",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "express": "^4.13.3",
    "jasmine-core": "^2.3.4",
    "karma-coverage": "^0.5.0",
    "karma-growl-reporter": "^0.1.1",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-diff-reporter": "^0.3.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "phantomjs-bin": "^1.0.1",
    "phantomjs-polyfill": "0.0.1",
    "react-addons-test-utils": "^0.14.0",
    "react-router": "1.0.0-rc1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "redux-logger": "^2.0.3",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "history": "^1.12.3",
    "lodash": "^3.10.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
    "redux": "^3.0.2",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0"
  }
}
